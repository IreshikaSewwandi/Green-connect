<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_gradient">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="ContentDescription,ImageContrastCheck,MissingConstraints" />

    <View
        android:id="@+id/rectangle_18"
        android:layout_width="382dp"
        android:layout_height="627dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/rectangle_18"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.072" />

    <View
        android:id="@+id/rectangle_19"
        android:layout_width="298dp"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="74dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="379dp"
        android:background="@drawable/rectangle_19"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_18"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_18"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/rectangle_18"
        app:layout_constraintTop_toTopOf="@+id/rectangle_18"
        app:layout_constraintVertical_bias="0.042" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="62dp"
        android:layout_height="59dp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_18"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_18"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="@+id/rectangle_18"
        app:layout_constraintTop_toTopOf="@+id/rectangle_18"
        app:layout_constraintVertical_bias="0.028"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/hi_l_e_g_ba"
        android:layout_width="267dp"
        android:layout_height="47dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:lineSpacingExtra="4.5sp"
        android:text="@string/hi_l_e_g_ba"
        android:textAppearance="@style/hi_l_e_g_ba"
        android:textSize="14sp"
        android:translationY="-1.97sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_19"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_19"
        app:layout_constraintHorizontal_bias="0.225"
        app:layout_constraintStart_toStartOf="@+id/rectangle_19"
        app:layout_constraintTop_toTopOf="@+id/rectangle_19"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="77dp"
        android:layout_height="81dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.952"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle_18"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ellips" />

    <View
        android:id="@+id/rectangle_20"
        android:layout_width="362dp"
        android:layout_height="39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/rectangle_20"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.448"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle_18"
        app:layout_constraintVertical_bias="0.438" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_20"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_20"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="@+id/rectangle_20"
        app:layout_constraintTop_toTopOf="@+id/rectangle_20"
        app:srcCompat="@drawable/greenmic" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_20"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_20"
        app:layout_constraintHorizontal_bias="0.946"
        app:layout_constraintStart_toStartOf="@+id/rectangle_20"
        app:layout_constraintTop_toTopOf="@+id/rectangle_20"
        app:layout_constraintVertical_bias="0.437"
        app:srcCompat="@drawable/camera" />

    <TextView
        android:id="@+id/type_your_q"
        android:layout_width="202dp"
        android:layout_height="14dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="@string/type_your_q"
        android:textAppearance="@style/type_your_q"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_20"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_20"
        app:layout_constraintStart_toStartOf="@+id/rectangle_20"
        app:layout_constraintTop_toTopOf="@+id/rectangle_20" />


</androidx.constraintlayout.widget.ConstraintLayout>